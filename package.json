{
  "name": "edge-workspaces",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "private": true,
  "scripts": {
    "watch": "node ./node_modules/webpack/bin/webpack.js --mode=development --watch --config config/webpack.config.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --mode=production --config config/webpack.config.js",
    "test": "jest .",
    "test-unit": "jest --testMatch='**/unit/*.test.[tj]s'",
    "test-e2e": "jest --testMatch='**/e2e/*.test.[tj]s'",
    "jest-unit": "jest --testMatch='**/unit/*.test.[tj]s'"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.248",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.3",
    "@types/uuid": "^9.0.7",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^0.10.1",
    "puppeteer": "^21.5.2",
    "size-plugin": "^2.0.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.9.0"
  }
}
