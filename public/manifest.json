{
  "manifest_version": 3,
  "name": "Threshold Tabs",
  "version": "0.1.0",
  "description": "Threshold Tabs is a Chrome extension that helps you manage your tabs .",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_title": "Threshold Tabs",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabGroups"
  ]
}